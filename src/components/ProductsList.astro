---
// Import statement if needed

const { title } = Astro.props;

// Fetch product data from the API
const products = await fetch('https://fakestoreapi.com/products').then(r => r.json());
---

<html>
<head>
  <title>Products</title>
</head>
<body>
  <h1>Products List</h1>
  <ul>
    {products.map(product => (
      <li>
        <h2>{product.title}</h2>
      </li>
    ))}
  </ul>
</body>
</html>
